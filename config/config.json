{
  "$schema": "https://dev.office.com/json-schemas/spfx-build/config.2.0.schema.json",
  "version": "2.0",
  "bundles": {
    "rmsdashboard-webpart-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/rmsdashboardWebpart/RmsdashboardWebpartWebPart.js",
          "manifest": "./src/webparts/rmsdashboardWebpart/RmsdashboardWebpartWebPart.manifest.json"
        }
      ]
    },
    "rms-postions-webpart-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/rmsPostionsWebpart/RmsPostionsWebpartWebPart.js",
          "manifest": "./src/webparts/rmsPostionsWebpart/RmsPostionsWebpartWebPart.manifest.json"
        }
      ]
    },
    "rms-application-sources-chart-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/rmsApplicationSourcesChart/RmsApplicationSourcesChartWebPart.js",
          "manifest": "./src/webparts/rmsApplicationSourcesChart/RmsApplicationSourcesChartWebPart.manifest.json"
        }
      ]
    },
    "dashboard-stats-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/dashboardStats/DashboardStatsWebPart.js",
          "manifest": "./src/webparts/dashboardStats/DashboardStatsWebPart.manifest.json"
        }
      ]
    },
    "real-time-list-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/realTimeList/RealTimeListWebPart.js",
          "manifest": "./src/webparts/realTimeList/RealTimeListWebPart.manifest.json"
        }
      ]
    },
    "sender-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/senderWp/SenderWpWebPart.js",
          "manifest": "./src/webparts/senderWp/SenderWpWebPart.manifest.json"
        }
      ]
    },
    "sample-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/sample/SampleWebPart.js",
          "manifest": "./src/webparts/sample/SampleWebPart.manifest.json"
        }
      ]
    }
  },
  "externals": {
    "jquery": {
      "path": "node_modules/jquery/dist/jquery.min.js",
      "globalName": "jQuery"
    },
    "bootstrap": {
      "path": "node_modules/bootstrap/dist/js/bootstrap.min.js",
      "globalName": "bootstrap"
    }
  },
  "localizedResources": {
    "RmsdashboardWebpartWebPartStrings": "lib/webparts/rmsdashboardWebpart/loc/{locale}.js",
    "RmsPostionsWebpartWebPartStrings": "lib/webparts/rmsPostionsWebpart/loc/{locale}.js",
    "RmsApplicationSourcesChartWebPartStrings": "lib/webparts/rmsApplicationSourcesChart/loc/{locale}.js",
    "DashboardStatsWebPartStrings": "lib/webparts/dashboardStats/loc/{locale}.js",
    "RealTimeListWebPartStrings": "lib/webparts/realTimeList/loc/{locale}.js",
    "senderWpStrings": "lib/webparts/senderWp/loc/{locale}.js",
    "SampleWebPartStrings": "lib/webparts/sample/loc/{locale}.js"
  }
}
